<div class="page-header">
  <h1>
    <%= User.find_by("id").username.camelize %>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">

      <% @photos.where(:user_id => @user.id).each do |post| %>
      <tr>
        <td><%= post.image %></td>
        <td><%= post.caption %></td>
        <td><%= post.likes.count %></td>
        <td><%= post.comments %></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
